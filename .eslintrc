{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ],
    "no-nested-ternary": 0,
    "react/button-has-type": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/forbid-prop-types": 0
  }
}